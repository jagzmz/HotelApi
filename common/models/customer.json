{
  "name": "Customer",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "favourites": {
      "type": [
        "string"
      ]
    },
    "location": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "bookings": {
      "type": "hasMany",
      "model": "Booking",
      "foreignKey": "custId"
    },
    "recommendations": {
      "type": "hasMany",
      "model": "Recommendation",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
